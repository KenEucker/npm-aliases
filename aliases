# Aliases

# NPM
alias n0="rm -rf node_modules package-lock.json yarn.lock"
alias nit="npm init"
alias na="npm audit"
alias ni="npm install"
alias nis="npm install --save"
alias nid="npm install --save-dev"
alias nig="npm install --global"
alias net="npm test"
alias nup="npm update"
alias nor="npm uninstall"
alias nr="npm run"
alias nrb="npm run build"
alias nrs="npm run start"
alias nrr="npm run run"
alias nodi="node index"
alias n0ni="n0 && ni"

## Yarn
alias yit="yarn init"
alias ya="yarn audit"
alias yi="yarn install"
alias yr="yarn run"
alias yrr="yarn run run"
alias yrb="yarn run build"
alias yrs="yarn run start"
alias yis="yarn add"
alias yid="yarn add --dev"
alias yig="yarn global add"
alias yet="yarn test"
alias yall="yarn-upgrade-all"
alias yor="yarn remove"
alias n0yi="n0 && yi"

# Git
alias go="git clone"
alias get="git checkout"
alias gm="git checkout master"
alias gmd="git merge develop"
alias gd="git checkout develop"
alias gen="git checkout -b"
alias gp="git pull"
alias gs="git stash"
alias gsa="git stash apply"
alias grh="git reset --hard HEAD"
