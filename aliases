# Aliases

#Node package management aliases
alias n0="rm -rf node_modules package-lock.json yarn.lock"
alias nit="npm init"
alias na="npm audit"
alias ni="npm install"
alias nis="npm install --save"
alias nid="npm install --save-dev"
alias nig="npm install --global"
alias nif="npm install --force"
alias nip="npm install --legacy-peer-deps"
alias net="npm test"
alias nup="npm update"
alias nor="npm uninstall"
alias nr="npm run"
alias nrc="npm run compile"
alias nrd="npm run dev"
alias nrb="npm run build"
alias nrs="npm run start"
alias nrr="npm run run"
alias nrf="npm run fix"
alias nrl="npm run lint"
alias nodi="node index"
alias n0ni="n0 && ni"
alias nin="npm install -g npm"

#PNPM package management aliases
alias p0="rm -rf node_modules pnpm-lock.yaml"
alias pit="pnpm init"
alias pa="pnpm audit"
alias pi="pnpm install"
alias pis="pnpm install --save"
alias pid="pnpm install --save-dev"
alias pig="pnpm install --global"
alias pif="pnpm install --force"
alias pip="pnpm install --legacy-peer-deps"
alias pet="pnpm test"
alias pup="pnpm update"
alias por="pnpm uninstall"
alias pr="pnpm run"
alias prd="pnpm run dev"
alias prb="pnpm run build"
alias prs="pnpm run start"
alias prr="pnpm run run"
alias prf="pnpm run fix"
alias prl="pnpm run lint"
alias podi="node index"
alias p0ni="n0 && ni"

## Yarn
alias yit="yarn init"
alias ya="yarn audit"
alias yi="yarn install"
alias yr="yarn run"
alias yis="yarn add"
alias yid="yarn add --dev"
alias yig="yarn global add"
alias yet="yarn test"
alias yall="yarn-upgrade-all"
alias yor="yarn remove"
alias yrb="yarn build"
alias yrs="yarn serve"
alias yrd="yarn dev"
alias nrl="yarn lint"
alias n0yi="n0 && yi"

# Git
alias go="git clone"
alias get="git checkout"
alias gtp="git checkout production"
alias gm="git checkout main"
alias gd="git checkout develop"
alias gen="git checkout -b"
alias gel="git checkout -"
alias gmd="git merge develop"
alias gad="git add ."
alias gor="git restore ."
alias gmm="git merge main"
alias gp="git pull"
alias gpc="git push"
alias gs="git stash"
alias gsa="git stash apply"

# Vagrant
alias vup="vagrant up"
alias vupp="vagrant up --provision"
alias vdown="vagrant halt"
alias var="vagrant reload"
alias vpi="vagrant plugin install"
alias vsh="vagrant ssh"

# Apt
alias si="sudo apt-get install"
alias sup="sudo apt-get update"

# Docker
alias dcud="docker-compose up -d"
alias dcu="docker-compose up"
alias dcd="docker-compose down"
alias dcf="docker-compose rm -fsv"
